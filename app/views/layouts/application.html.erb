<!DOCTYPE html>
<html>
<head>
  <title>Students</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %> 
  <%= csrf_meta_tags %>
  
  <nav class="navbar navbar-inverse">
  <div class="container-fluid">
    <div class="navbar-header">
      <a class="navbar-brand" href="<%= students_path %>">Welcome</a>
    </div>
          <ul class="nav navbar-nav">
            <li class="active"><a href="<%= students_path %>">Students</a></li>
            <li ><a href="<%= results_path %>">Results</a></li>
           
          </ul>
    <ul class="nav navbar-nav navbar-right">
            <% if user_signed_in? %>
            <li><%= link_to current_user.email, edit_user_registration_path %></li>
            <li><%= link_to "Logout", destroy_user_session_path, method: :delete %></li>
            <% else %>
            <li ><a href="<%= new_user_registration_path %>">Sign up</a></li>
            <li ><a href="<%= new_user_session_path %>">Login</a></li>
            <% end %>
          </ul>
      
        
  </div>
  </nav>
</head>
    
<body class="container" style="background-color:	#cccccc">
<%= yield %>

</body>
  
  
</html>